// 2798. Number of Employees Who Met the Target
// leetcode solution


class Solution {
    public int numberOfEmployeesWhoMetTarget(int[] hours, int target) {
        int employee = 0;
        for(int i=hours.length-1;i>=0;i--){
            if(hours[i]>=target){
                employee++;
            }
        }
        return employee;
    }
}
